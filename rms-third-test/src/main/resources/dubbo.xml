<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="demo-provider"/>
    <dubbo:registry id="rms-third-zookeeper" protocol="zookeeper"  address="127.0.0.1:2181" group="rms-third-service"/>
    <dubbo:protocol name="dubbo" port="20880"/>
    <dubbo:service registry="rms-third-zookeeper"
                   timeout="20000"  interface="com.jzfq.rms.third.test.DemoService" ref="demoService" protocol="dubbo" />
    <bean id="demoService" class="com.jzfq.rms.third.test.impl.DemoServiceImpl"/>
</beans>